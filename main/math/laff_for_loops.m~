function [] = laff_for_loops( ops , loop_length )
%One for loop for many operations


fileID = fopen('user_laff_func_gen.cpp','w');


fprintf(fileID, 'for(i=0; i<%d; i++){ \n', loop_length);
fprintff
for j=1:length(ops)
    
    fprintf(fileID, strcat('\t', sprintf(ops{j}), '\n'));
    
end

fprintf(fileID, '}');



end

